{
  "name": "create-neotoc",
  "version": "0.1.0",
  "description": "Neotoc playground powered by vite and dummy-content",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --minify --format esm",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "eslint src/",
    "lint-fix": "eslint src/ --fix",
    "check-format": "prettier src/ --check",
    "format": "prettier src/ --write"
  },
  "keywords": [
    "neotoc",
    "table of contents",
    "playground"
  ],
  "author": "Ashutosh Biswas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashutoshbw/neotoc.git",
    "directory": "packages/create-neotoc"
  },
  "bugs": "https://github.com/ashutoshbw/neotoc/issues",
  "homepage": "https://neotoc.vercel.app",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.1",
    "prettier": "3.5.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.3",
    "dummy-content": "^0.1.0",
    "node-html-parser": "^7.0.1",
    "picocolors": "^1.1.1"
  }
}
